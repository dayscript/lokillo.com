        "cweagans/composer-patches": "^1.6.0",
        "drupal/admin_toolbar": "^1.24",
        "drupal/adminimal_theme": "^1.3",
        "drupal/backup_migrate": "^4.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_class": "^1.0",
        "drupal/calendar": "1.x-dev",
        "drupal/colorbox": "^1.4",
